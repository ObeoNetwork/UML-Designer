            <controls xsi:type="properties:DynamicMappingForDescription" name="PR_AssociationEndsFor" iterator="end" iterableExpression="aql:self.getAssociationEnds()" forceRefresh="true">
              <ifs name="isVisibleAssociationEndLabel" predicateExpression="aql:self.oclIsKindOf(uml::Association)">
                <widget xsi:type="properties:HyperlinkDescription" name="AssociationEndHyperLink" valueExpression="aql:end" displayExpression="aql:end.name">
                  <initialOperation>
                    <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="var:end">
                      <subModelOperations xsi:type="properties:DialogModelOperation" titleExpression="Edit">
                        <buttons labelExpression="Cancel" closeDialogOnClick="true" rollbackChangesOnClose="true">
                          <initialOperation/>
                        </buttons>
                        <buttons labelExpression="OK" default="true" closeDialogOnClick="true">
                          <initialOperation/>
                        </buttons>
                        <page name="Default Page" labelExpression="aql:self.computeUmlLabel()" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.0"/>
                      </subModelOperations>
                    </firstModelOperations>
                  </initialOperation>
                </widget>
              </ifs>
              <ifs name="isVisibleAssociationEndNavigability" predicateExpression="aql:self.oclIsKindOf(uml::Association)">
                <widget xsi:type="properties:CheckboxDescription" name="AssociationEndNavigable" labelExpression="Navigable" valueExpression="aql:end.isNavigableEnd()">
                  <initialOperation>
                    <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:end.setIsNavigableEnd(newValue)"/>
                  </initialOperation>
                </widget>
              </ifs>
              <ifs name="isVisibleAssociationEndOwner" predicateExpression="aql:self.oclIsKindOf(uml::Association)">
                <widget xsi:type="properties:CheckboxDescription" name="AssociationEndOwner" labelExpression="Owned" valueExpression="aql:end.oclAsType(uml::Property).owner.oclIsKindOf(uml::Association)">
                  <initialOperation>
                    <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:end.setIsOwnedEnd(newValue)"/>
                  </initialOperation>
                </widget>
              </ifs>
              <ifs name="isVisibleAssociationKind" predicateExpression="aql:self.oclIsKindOf(uml::Association)">
                <widget xsi:type="properties:SelectDescription" name="AssociationEndKindSelection" valueExpression="aql:end.aggregation" candidatesExpression="aql:end.getAggregationKinds()" candidateDisplayExpression="aql:candidate.getAggregationKindName()">
                  <initialOperation>
                    <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:end.setAggregationKind(newValue)"/>
                  </initialOperation>
                </widget>
              </ifs>
              <ifs name="isVisibleAssociationEndLowerBound" predicateExpression="aql:self.oclIsKindOf(uml::Association)">
                <widget xsi:type="properties:TextDescription" name="AssociationEndLowerBound" labelExpression="Lower:" valueExpression="aql:end.getLower().getBoundValue()">
                  <initialOperation>
                    <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:end.setPropertyLowerBound(end.getLower(), newValue)"/>
                  </initialOperation>
                </widget>
              </ifs>
              <ifs name="isVisibleAssociationEndUpperBound" predicateExpression="aql:self.oclIsKindOf(uml::Association)">
                <widget xsi:type="properties:TextDescription" name="AssociationEndLowerBound" labelExpression="Upper:" valueExpression="aql:end.getUpper().getBoundValue()">
                  <initialOperation>
                    <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:end.setPropertyUpperBound(end.getUpper(), newValue)"/>
                  </initialOperation>
                </widget>
              </ifs>
            </controls>
            <layout xsi:type="properties:GridLayoutDescription" numberOfColumns="6"/>
          </controls>
        </controls>
        <actions tooltipExpression="Add a new association end" imageExpression="/org.obeonetwork.dsl.uml2.design/icons/properties/create.gif">
          <initialOperation>
            <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.addAssociationEnd()"/>
          </initialOperation>
        </actions>
        <actions tooltipExpression="Remove ends" imageExpression="/org.obeonetwork.dsl.uml2.design/icons/properties/unset.gif">
          <initialOperation>
            <firstModelOperations xsi:type="tool:ChangeContext" browseExpression="aql:self.removeAssociationEnd()"/>
          </initialOperation>
        </actions>
