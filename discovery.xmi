<?xml version="1.0" encoding="ASCII"?>
<discovery:DiscoveryDefinition
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:discovery="http://www.eclipse.org/amalgamation/discovery/1.0">
  <categories
      name="Designers"
      description="Extended designers."
      relevance="6"
      image48="">
    <components
        name="SysML Designer"
        provider="Obeo Network"
        description="SysML Designer provides a set of common diagrams to work with SysML models."
        license="EPL"
        image32="images/sysml_designer.png">
      <sitesURLS>http://marketplace.obeonetwork.com/updates/sirius2.0</sitesURLS>
      <overview
          summary="SysML Designer is a graphical tool to edit and vizualize SysML 1.3 models. As it is based on Sirius, it provides an easy way to combine SysML with domain specific modelling. You can extend the provided diagram definitions and seamlessly work on both SysML and DSL models at the same time. "
          screenshot="images/sysml_designer_screenshot-320x240.png"
          url="http://marketplace.obeonetwork.com/module/sysml"/>
      <id>org.obeonetwork.dsl.sysml.runtime.feature</id>
    </components>
  </categories>
  <categories
      name="Generators"
      description="UML generators."
      image48="">
    <components
        name="UML to Java"
        provider="Obeo Network"
        description="The UML to Java generator provides an Acceleo generator to produce Java from an UML model."
        license="EPL"
        image32="images/Uml2Java.png">
      <sitesURLS>http://download.eclipse.org/umlgen/updates/releases/1.0.0/mars</sitesURLS>
      <overview
          summary="This generator allows you to produce Java source code from a UML model. This module uses the standard UML2 metamodel provided by Eclipse Foundation and as such is compatible with any UML tools using this standard like the UML Designer. It's based on the award winning code generation project, Acceleo."
          screenshot="images/uml2java-screenshot-320x240.png"
          url="https://wiki.eclipse.org/Eclipse_UML_Generators"/>
      <id>org.eclipse.umlgen.java.ui.feature</id>
      <id>org.eclipse.umlgen.java.feature</id>
    </components>
  </categories>
  <filters name="Incubation"/>
</discovery:DiscoveryDefinition>
