------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

// Delete Interface Realization by selecting Interface realization link
with [get-editor "test Class Diagram"] {
    with [get-diagram -index 1 | get-edit-part -name "test Class Diagram"] {
        with [get-edit-part -name Interface1 | get-target-connection 0] {
            mouse-move 162 27 button1 -height 44 -width 209
            mouse-press 162 27 button1 -height 44 -width 209
            mouse-release 164 28 button1 524288 -height 47 -width 213
        }
    }
    get-button "Delete from Model" | click
}
// Check diagram
get-editor "test Class Diagram" | get-diagram -index 1 | get-edit-part -name "test Class Diagram" 
    | get-edit-part -name Interface1 | get-target-connection 0
get-editor "test Class Diagram" | get-diagram -index 1 | get-edit-part -name "test Class Diagram" 
    | get-edit-part -name Interface1 | get-target-connection 1
get-editor "test Class Diagram" | get-diagram -index 1 | get-edit-part -name "test Class Diagram" 
    | get-edit-part -name Interface1 | verify-error{get-target-connection 2}
//Check model
with [get-editor "test Class Diagram" | get-diagram -index 1 | get-edit-part -name "test Class Diagram"] {
    get-property "element.edges.size" | equals 2 | verify-true
    get-property "element.edges.DEdge[0].semanticElements.InterfaceRealization[0].source.Class[0].name" | equals Class5 
        | verify-true
    get-property "element.edges.DEdge[0].semanticElements.InterfaceRealization[0].target.Interface[0].name" 
        | equals Interface1 | verify-true
    get-property "element.edges.DEdge[1].semanticElements.InterfaceRealization[0].source.Class[0].name" | equals Class4 
        | verify-true
    get-property "element.edges.DEdge[1].semanticElements.InterfaceRealization[0].target.Interface[0].name" 
        | equals Interface1 | verify-true
    verify-error{get-property "element.target.ownedMember.Class[0].interfaceRealization.InterfaceRealization[0].target.Interface[0].name" 
        | equals Interface1 | verify-true}
    get-property "element.target.ownedMember.Class[3].interfaceRealization.InterfaceRealization[0].target.Interface[0].name" 
        | equals Interface1 | verify-true
    get-property "element.target.ownedMember.Class[4].interfaceRealization.InterfaceRealization[0].target.Interface[0].name" 
        | equals Interface1 | verify-true
}

// Delete class source which is of interface realization
with [get-editor "test Class Diagram"] {
    with [get-diagram -index 1 | get-edit-part -name "test Class Diagram"] {
        with [get-edit-part -name Class4 | get-edit-part -className DNodeListViewNodeListCompartment2EditPart] {
            mouse-move 37 17 button1 -height 71 -width 116
            mouse-press 37 17 button1 -height 71 -width 116
            mouse-release 37 17 button1 524288 -height 71 -width 116
        }
    }
    get-button "Delete from Model" | click
}
// Check diagram
get-editor "test Class Diagram" | get-diagram -index 1 | get-edit-part -name "test Class Diagram" 
    | get-edit-part -name Interface1 | get-target-connection 0
get-editor "test Class Diagram" | get-diagram -index 1 | get-edit-part -name "test Class Diagram" 
    | get-edit-part -name Interface1 | verify-error{get-target-connection 1}
//Check model

with [get-editor "test Class Diagram" | get-diagram -index 1 | get-edit-part -name "test Class Diagram"] {
    get-property "element.target.ownedMember.size" | equals 5 | verify-true
    get-property "element.target.ownedMember.Class[0].name" | equals Class1 | verify-true
    get-property "element.target.ownedMember.Class[1].name" | equals Class2 | verify-true
    get-property "element.target.ownedMember.Class[2].name" | equals Class3 | verify-true
    get-property "element.target.ownedMember.Class[3].name" | equals Class5 | verify-true
    get-property "element.target.ownedMember.Class[3].interfaceRealization.size" | equals 1 | verify-true
    get-property "element.target.ownedMember.Class[3].interfaceRealization.InterfaceRealization[0].owner.name" 
        | equals Class5 | verify-true
    get-property "element.target.ownedMember.Class[3].interfaceRealization.InterfaceRealization[0].source.Class[0].name" 
        | equals Class5 | verify-true
    get-property "element.target.ownedMember.Class[3].interfaceRealization.InterfaceRealization[0].target.Interface[0].name" 
        | equals Interface1 | verify-true
    get-property "element.target.ownedMember.Class[3].interfaceRealization.InterfaceRealization[0].client.Class[0].name" 
        | equals Class5 | verify-true
    get-property "element.target.ownedMember.Class[3].interfaceRealization.InterfaceRealization[0].supplier.Interface[0].name" 
        | equals Interface1 | verify-true
    get-property "element.target.ownedMember.Interface[4].name" | equals Interface1 | verify-true
}

// Delete Interface which is the target of an interface realization
with [get-editor "test Class Diagram"] {
    with [get-diagram -index 1 | get-edit-part -name "test Class Diagram"] {
        with [get-edit-part -name Interface1 | get-edit-part -className DNodeListViewNodeListCompartment2EditPart] {
            mouse-move 74 11 button1 -height 71 -width 116
            mouse-press 74 11 button1 -height 71 -width 116
            mouse-release 74 11 button1 524288 -height 71 -width 116
        }
    }
    get-button "Delete from Model" | click
}
// Check diagram
get-editor "test Class Diagram" | get-diagram -index 1 | get-edit-part -name "test Class Diagram" 
    | verify-error{get-edit-part -name Interface1}
//Check model
with [get-editor "test Class Diagram" | get-diagram -index 1 | get-edit-part -name "test Class Diagram"] {
    get-property "element.target.ownedMember.size" | equals 4 | verify-true
    get-property "element.target.ownedMember.Class[0].name" | equals Class1 | verify-true
    get-property "element.target.ownedMember.Class[1].name" | equals Class2 | verify-true
    get-property "element.target.ownedMember.Class[2].name" | equals Class3 | verify-true
    get-property "element.target.ownedMember.Class[3].name" | equals Class5 | verify-true
    get-property "element.target.ownedMember.Class[3].interfaceRealization.size" | equals 0 | verify-true
}
