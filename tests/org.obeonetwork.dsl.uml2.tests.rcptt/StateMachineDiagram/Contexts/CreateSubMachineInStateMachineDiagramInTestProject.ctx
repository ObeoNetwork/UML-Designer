// create a SubMachine in State Machine diagram Region1
with [get-editor "NewModel State Machine Diagram"] {
    with [get-palette | get-palette-entry Submachine] {
            mouse-move 62 10 button1 -height 23 -width 134
            mouse-press 62 10 button1 -height 23 -width 134
            mouse-release 62 10 button1 524288 -height 23 -width 134
    }
    with [get-diagram -index 1 | get-edit-part -name "NewModel State Machine Diagram" | get-edit-part -name NewModel 
        | get-edit-part -className DNodeContainerViewNodeContainerCompartmentEditPart | get-edit-part -name Region1 
        | get-edit-part -className DNodeContainerViewNodeContainerCompartment2EditPart] {
        mouse-move 63 366 button1 -height 481 -width 571
        mouse-press 63 366 button1 -height 481 -width 571
        mouse-release 63 366 button1 524288 -height 481 -width 571
    }
}
