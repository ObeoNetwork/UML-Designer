//Create Deployment between Artifact1 and ExecutionEnvironment1
with [get-editor "NewModel Deployment Diagram"] {
    with [get-palette | get-palette-entry Deployment] {
        mouse-move 29 8 button1 -height 23 -width 214
        mouse-press 29 8 button1 -height 23 -width 214
        mouse-release 29 8 button1 524288 -height 23 -width 214
    }
    with [get-diagram -index 1] {
        with [get-edit-part -name "NewModel Deployment Diagram" 
            | get-edit-part -name Artifact1 | get-edit-part 
                -className DNodeContainerViewNodeContainerCompartmentEditPart] {
            mouse-move 56 19 button1 -height 38 -width 118
            mouse-press 56 19 button1 -height 38 -width 118
        }
        with [get-edit-part -name "NewModel Deployment Diagram" 
            | get-edit-part -name ExecutionEnvironment1 | get-edit-part 
            -className DNodeContainerViewNodeContainerCompartmentEditPart] {
            mouse-move 53 18 button1 -height 40 -width 155
            mouse-press 53 18 button1 -height 40 -width 155
            mouse-release 53 18 button1 524288 -height 40 -width 155
        }
    }
}
