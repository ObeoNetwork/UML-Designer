Content-Type: text/ecl
Entry-Name: .content

// Create new Interface
with [get-editor "NewModel Use Case Diagram"] {
    with [get-palette | get-palette-entry Interface] {
            mouse-move 54 6 button1 -height 23 -width 146
            mouse-press 54 6 button1 -height 23 -width 146
            mouse-release 54 6 button1 524288 -height 23 -width 146
    }
    with [get-diagram -index 1 | get-edit-part -name "NewModel Use Case Diagram"] {
        mouse-move 592 74 button1 -height 530 -width 781
        mouse-press 592 74 button1 -height 530 -width 781
        mouse-release 592 74 button1 524288 -height 530 -width 781
    }
}

// Check Diagram
with [get-editor "NewModel Use Case Diagram" | get-diagram -index 1 | get-edit-part -name "NewModel Use Case Diagram" 
    | get-edit-part -name "NewModel Subject" | get-edit-part 
    -className DNodeContainerViewNodeContainerCompartmentEditPart | get-figure "1/0"] {
    get-property "isVisible()" | equals true | verify-true
}
