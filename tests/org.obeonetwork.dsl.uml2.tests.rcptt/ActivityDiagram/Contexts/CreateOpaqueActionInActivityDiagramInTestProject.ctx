// Create an Opaque action in activity diagram in test project
with [get-editor "NewModel Activity Diagram"] {
    with [get-palette | get-palette-entry "Opaque Action"] {
            mouse-move 100 9 button1 -height 23 -width 214
            mouse-press 100 9 button1 -height 23 -width 214
            mouse-release 100 9 button1 524288 -height 23 -width 214
            mouse-move 9 21 -height 23 -width 214
    }
    with [get-diagram -index 1 | get-edit-part -name "NewModel Activity Diagram"] {
        with [get-edit-part -name "NewModel"] {
            with [get-edit-part -className DNodeContainerViewNodeContainerCompartmentEditPart] {
                mouse-move 127 115 button1 -height 332 -width 542
                mouse-press 127 115 button1 -height 332 -width 542
                mouse-move 128 115 -mask 524288 -height 332 -width 542
            }
            mouse-drag 312 232 button1 524288 -height 334 -width 544
            mouse-release 312 232 button1 524288 -height 334 -width 544
        }
    }
